{
  "name": "metaco_backend",
  "version": "1.0.0",
  "description": "backend service for metaco test",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --forceExit  --coverage",
    "dev": "nodemon bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate-test": "npx sequelize db:migrate --env test",
    "seed": "npx sequelize-cli db:seed:all",
    "start": "node bin/www"
  },
  "author": "halim syarif",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "sequelize": "^6.25.1",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "convert-csv-to-json": "^1.3.3",
    "jest": "^29.1.2",
    "npm": "^8.19.2",
    "sequelize-cli": "^6.5.1",
    "supertest": "^6.3.0"
  }
}
