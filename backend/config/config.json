{
  "development": {
    "dialect": "sqlite",
    "storage": "./db/metaco.db"
  },
  "test": {
    "dialect": "sqlite",
    "storage": "./db/metaco_test.db"
  },
  "production": {
    "use_env_variable":"DATABASE_URL",
    "ssl": true,
    "dialect": "postgres",
    "protocol": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
